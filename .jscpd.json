{
  "threshold": 0.26,
  "reporters": ["html", "console"],
  "ignore": [
    "**/webpack.config.js",
    "**/webpack/**",
    "**/*.sh",
    "**/node_modules/**",
    "**/__snapshots__/**",
    "**/__fixtures__/**",
    "**/fixtures/**",
    "**/*.html",
    "**/*.spec.ts",
    "**/spec.ts",
    "**/i18n/*.json",
    "**/*.svg",
    "**/Deprecated/*.ts",
    "**/Deprecated/**/index.ts",
    "**/coverage/**",
    "**/reports/**",
    "**/dist/**",
    "**/server/**",
    "**/*.feature",
    "**/README.md",
    "**/bin/**",
    "**/docs",
    "**/apps/front/client/pages/Static/A11y/UK.tsx",
    "**/apps/front/client/pages/Static/A11y/CS.tsx",
    "**/apps/front/client/pages/Static/A11y/EN.tsx",
    "**/apps/front/client/pages/Static/Data/UK.tsx",
    "**/cypress/support/step_definitions/proposal_submit.js",
    "**/types/**",
    "**/package/logger/**",
    "**/package/store/index.tsx",
    "**/apps/assembly-ui/store/context.tsx"
  ],
  "absolute": true,
  "output": "./reports",
  "minTokens": 100
}
