{
  "threshold": 0.26,
  "reporters": ["html", "console"],
  "ignore": [
    "**/webpack.config.js",
    "**/webpack/**",
    "**/*.sh",
    "**/node_modules/**",
    "**/__snapshots__/**",
    "**/__fixtures__/**",
    "**/fixtures/**",
    "**/*.html",
    "**/*.spec.ts",
    "**/spec.ts",
    "**/i18n/*.json",
    "**/*.svg",
    "**/Deprecated/*.ts",
    "**/Deprecated/**/index.ts",
    "**/coverage/**",
    "**/reports/**",
    "**/dist/**",
    "**/server/**",
    "**/*.feature",
    "**/README.md",
    "**/bin/**",
    "**/apimock/db/deprecatedHomePageQuestion.json",
    "**/apimock/db/deprecatedQuestion.json",
    "**/apimock/db/deprecatedViews.json"
  ],
  "absolute": true,
  "output": "./reports",
  "minTokens": 100
}
