            "agree": 38,
            "like_it": 13,
            "disagree": 50,
            "no_way": 17,
            "votes": 151
        },{
            "id": "9cb58069-56cb-45cf-ae81-91e9c9f0b974",
            "content": "Il faut rendre accessibles les véhicules électriques à tout le monde et surtout à ceux qui vivent à la campagne",
            "author": "sifdin",
            "agree": 45,
            "like_it": 7,
            "disagree": 36,
